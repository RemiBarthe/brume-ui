{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^vue",
    "^@?\\w",
    "^@/assets/(.)$",
    "^@/components/(.)$",
    "^@/composables/(.)$",
    "^@/pages/(.)$",
    "^@/utils/(.*)$",
    "^[./]"
  ]
}